<form method="post" action="{{ path('app_servicebooking_delete', {id: servicebooking.id}) }}" onsubmit="return confirm('Are you sure you want to delete this booking?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ servicebooking.id) }}">
    <button class="btn btn-sm btn-danger">Delete</button>
</form>
